<div class="container">
  <h1>Chat Room by Action Cable</h1>
  <form class="form-inline">
    <div class='form-group'>
      <input type="text" class="form-control" id='chat-input'/>
      <button class="btn btn-primary" id='button'>送信</button>
    </div>
  </form>
  <div id="messages">
    <ul class="list-group">
    <%= render @messages %>
    </ul>
  </div>
</div>